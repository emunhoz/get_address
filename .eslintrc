{
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "plugins": [
        "import",
        "prettier",
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "space-before-function-paren": 0,
        "new-cap": 0,
        "prettier/prettier": 2
    }
}